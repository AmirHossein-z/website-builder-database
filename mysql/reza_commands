create table template (ID int(10) primary key not null, name varchar(255), date_create date, date_last_update varchar(255), categoryID int(10), foreign key(categoryID) references category(ID));

create table category (ID int(10) primary key not null, title varchar(255) not null);

create table picture (name varchar(255), address varchar(255), templateID int(10), foreign key(templateID) references template(ID));

create table wallet (ID int(10) primary key not null, inventory int(10), customerID int(10), foreign key(customerID) references customer(ID));

create table employee (ID int(10) primary key not null, first_name varchar(255), last_name varchar(255), phone_number varchar(11), national_code varchar(10), email varchar(50), type int(10), buyID int(10), foreign key (buyID) references buy(ID));

create table transaction (ID int(10) primary key not null, date date, price int(10), type int(1), status int(1), walletID int(10), customerID int(10), foreign key (walletID) references wallet(ID), foreign key (customerID) references customer(ID));

